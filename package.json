{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "concurrently \"npm run prebuild\" \"astro build\"",
    "prebuild": "node scripts/generate.cjs --DEFAULT_LANG uk --AVAILABLE_LANGS en --AVAILABLE_LANGS ru --AVAILABLE_LANGS uk",
    "preview": "concurrently \"run-s preview:*\"",
    "preview:build": "astro build",
    "preview:wrangler": "concurrently \"kill-port 8788\" \"wrangler pages dev ./dist --local --persist --port 8788 --kv DANCE_KV --env PROXY_API_ARL=/\"",
    "dev": "concurrently \"cross-env NODE_ENV=development run-s dev:*\"",
    "dev:build": "astro build",
    "dev:server": "concurrently \"kill-port 3000,8788\" \"wrangler pages dev ./dist --local --persist --port 8788 --kv DANCE_KV --env PROXY_API_ARL=/  --compatibility-flag streams_enable_constructors\" \"astro dev --port 3000\"",
    "test": " concurrently \"wrangler pages dev ./dist --local --persist --port 8788 --kv DANCE_KV --env PROXY_API_ARL --compatibility-flag streams_enable_constructors\""
  },
  "dependencies": {
    "@astrojs/cloudflare": "^6.1.1",
    "@astrojs/prefetch": "^0.1.1",
    "@astrojs/react": "^2.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^3.0.0",
    "@nanostores/react": "^0.4.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "accept-language-parser": "^1.5.0",
    "astro": "^2.0.2",
    "nanostores": "^0.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@types/accept-language-parser": "^1.5.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "kill-port": "^2.0.1",
    "minimist": "^1.2.7",
    "npm-run-all": "^4.1.5",
    "wrangler": "^2.9.0"
  }
}
