---
import { getLang } from "../../../utils/url";
import Header from "./Header";
import type { HeaderType } from "./types";
let navigation = [];
try {
  navigation = await Astro.glob("../../../../content/navigation.md");
} catch (e) {}

const { header } = (navigation || [])[0]?.frontmatter || {};
const props: HeaderType = { navigation: header, url: Astro.url.href, lang: getLang(Astro.url), langs: ['uk', 'en'] };
---

<Header {...props} client:load />
